                                            <ColumnDefinition x:Name="IconRow" Width="*" />
                                            <ColumnDefinition x:Name="ContentRow" Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <Viewbox
                                            x:Name="IconBox"
                                            Width="{ThemeResource TopTabBarItemIconHeight}"
                                            Height="{ThemeResource TopTabBarItemIconHeight}">
                                            <ContentPresenter
                                                x:Name="Icon"
