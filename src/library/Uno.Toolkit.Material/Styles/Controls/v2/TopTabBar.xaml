	<SolidColorBrush x:Key="M3MaterialColoredTopTabBarItemOnPrimaryBrush"
	                 Opacity="0.60"
	                 Color="{ThemeResource OnPrimaryColor}" />


	<StaticResource x:Key="M3MaterialColoredTopTabBarItemBorderBrush"
	                ResourceKey="SystemControlTransparentBrush" />

	<x:Double x:Key="M3MaterialColoredTopTabBarFontSize">14</x:Double>
