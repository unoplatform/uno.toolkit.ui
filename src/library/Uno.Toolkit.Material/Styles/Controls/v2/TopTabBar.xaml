									</VisualState.Setters>
								</VisualState>
								<VisualState x:Name="Pressed">
									<VisualState.Setters>
										<Setter Target="LayoutRoot.Background"
										        Value="{ThemeResource MaterialTopTabBarItemBackgroundPressed}" />
										<Setter Target="PointerRectangle.Fill"
										        Value="{ThemeResource MaterialTopTabBarItemBackgroundPressed}" />
										<Setter Target="Icon.Foreground"
